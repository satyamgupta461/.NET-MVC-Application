@model List<Employee>
@Html.ActionLink("Create New Employee","Create","Employee");
    <table>
        <thead>
            <tr>
                <th>
                    Id
                </th>
                <th>
                    Name
                </th>
                <th>
                    Email
                </th>
                <th>
                    Dob
                </th>
                <th>
                    Address
                </th>
                <th>
                    PhoneNumber
                </th>
                <th>
                    City
                </th>

            </tr>

        </thead>
        <tbody>
        @foreach(var employee in Model)
        {
            <tr>
                <td>
                    @employee.Id
                </td>
                <td>
                    @employee.Name
                </td>
                <td>
                    @employee.Email 
                </td>
                <td>
                    @employee.Dob
                </td>
                <td>
                    @employee.Address
                </td>
                <td>
                    @employee.PhoneNumber
                </td>
                <td>
                    @employee.City
                </td>
                <td>
                    @Html.ActionLink("Edit","Edit", new {id = employee.Id})
                    @Html.ActionLink("Delete","Delete", new {id = employee.Id})
                </td>

            </tr>
        }
        </tbody>
    </table>
